<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="OlimpaSolution.models.DataControl">
        
            <TabControl x:Name="TabControl">
                <TabItem Header="Map">
                    <Image x:Name="MapImage"></Image>
                </TabItem>
                <TabItem Header="Diagrams">
                    <Grid x:Name="GridListBox">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                    </Grid>
                </TabItem>
                <TabItem x:Name="DataEnter" Header="User input">
                    <DockPanel><TextBox x:Name="A"></TextBox>
                        <TextBox x:Name="B"></TextBox></DockPanel>
                </TabItem>
            </TabControl>       

</UserControl>